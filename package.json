  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "nodemon": "^2.0.22",
    "sequelize-cli-typescript": "^3.2.0-c",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
